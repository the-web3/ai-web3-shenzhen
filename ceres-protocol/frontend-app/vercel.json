{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        },
        {
          "key": "Content-Security-Policy",
          "value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https://vercel.live; style-src 'self' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data:; connect-src 'self' https://testnet.hsk.xyz wss://testnet.hsk.xyz https://api.ceres-protocol.com wss://ws.ceres-protocol.com; frame-src 'none'; object-src 'none';"
        }
      ]
    }
  ],
  "env": {
    "VITE_CERES_GREEN_POINTS_ADDRESS": "0xafDC7e5818DFEe3F89922d603DFDDd610E39d17C",
    "VITE_CERES_REGISTRY_ADDRESS": "0xa2D68CB060de12d39eE9307204632fA2a7520046",
    "VITE_CERES_MARKET_FACTORY_ADDRESS": "0xAB19A0695CC9AdD80Bd321C96d5C8a2403e8265d",
    "VITE_HASHKEY_CHAIN_ID": "133",
    "VITE_HASHKEY_RPC_URL": "https://testnet.hsk.xyz",
    "VITE_NETWORK_NAME": "HashKey Chain Testnet",
    "VITE_EXPLORER_URL": "https://testnet-explorer.hsk.xyz",
    "VITE_APP_NAME": "Ceres Protocol",
    "VITE_APP_VERSION": "1.0.0",
    "VITE_ENABLE_AI_DEMO": "true"
  }
}
