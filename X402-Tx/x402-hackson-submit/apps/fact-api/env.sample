# HTTP server port
PORT=8787
# Self-hosted facilitator base URL
FACILITATOR_URL=http://localhost:8789
# EIP-3009 token address (payment asset)
PAYMENT_ASSET=0x0000000000000000000000000000000000000000
# CAIP-2 network id
PAYMENT_NETWORK=eip155:133
# x402 scheme (exact)
PAYMENT_SCHEME=exact
# Price in token smallest unit
PAYMENT_PRICE=1000000000000000000
# Payment recipient (defaults to attestor address)
PAYMENT_PAYEE=0x393ff885a6588f50ca951bb8f0de001cb30009f1
# Attestor ECDSA private key (0x + 64 hex)
ATTESTOR_PRIVATE_KEY=0xaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaaa
# Dev shortcut: skip payment verification/settlement
DEV_BYPASS_PAYMENT=false

