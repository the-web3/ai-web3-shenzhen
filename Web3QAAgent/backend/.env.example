# DeepSeek API 配置
DEEPSEEK_API_KEY=your-deepseek-api-key-here
DEEPSEEK_BASE_URL=https://api.deepseek.com
DEFAULT_MODEL=deepseek-chat

# MinerU API 配置 (PDF 解析)
MINERU_API_KEY=your-mineru-api-key-here
# 本地部署时可改为 http://localhost:8001/api/v4
MINERU_API_BASE=https://mineru.net/api/v4

# Embedding 配置（DashScope 或兼容 OpenAI 的本地服务）
DASHSCOPE_API_KEY=your-dashscope-api-key-here
# 本地部署时可改为 http://localhost:8002/v1
DASHSCOPE_BASE_URL=https://dashscope.aliyuncs.com/compatible-mode/v1
EMBEDDING_MODEL=text-embedding-v4

# 服务器配置
HOST=0.0.0.0
PORT=8000
DEBUG=true

# CORS 配置 (逗号分隔)
CORS_ORIGINS=http://localhost:5173,http://localhost:3000

# 缓存配置
CACHE_ENABLED=true
CACHE_DIR=./cache

# 知识库文档存储
KNOWLEDGE_STORE_PATH=./data/knowledge_store.json

# 向量数据库
CHROMA_PERSIST_DIR=./chroma_db
